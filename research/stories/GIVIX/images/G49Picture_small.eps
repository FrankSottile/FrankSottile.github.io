45 dict begin
gsave

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%    Draws box from (a,b) to (c,d) 
/BOX {/d exch def /c exch def /b exch def /a exch def 
        a b moveto c b lineto c d lineto a d lineto closepath} def
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%10 260 translate 2 2 scale 1 setlinewidth 1 0 1 setrgbcolor 0 0 181 150 BOX stroke

/step 15.38 def

/sqr {/y exch def /x exch def
      gsave
      x y translate
      newpath 0 0 moveto step 0 rlineto 0 step rlineto step neg 0 rlineto closepath fill
      grestore
     } def
/fixed {
%        0 setlinewidth
%        0.5 0 0 setrgbcolor
%        newpath -0.3 step mul -0.3 step mul 5.3 step mul 4.3 step mul BOX fill
	
 	1 0.2 0.2 setrgbcolor
	newpath 0 2 step mul moveto 3 step mul 0 rlineto 0 step rlineto  step 0 rlineto 0 step rlineto -4 step mul 0 rlineto closepath fill
	0 0 1 setrgbcolor
	newpath 2 step mul  0 moveto step 0 rlineto 0 2 step mul rlineto step neg 0 rlineto closepath fill	
	0 0.6 0 setrgbcolor
	newpath 3 step mul  0 moveto step 0 rlineto 0 2 step mul rlineto step neg 0 rlineto closepath fill	
        1 1 0 setrgbcolor
	newpath 4 step mul 0 moveto step 0 rlineto 0 3 step mul rlineto step neg 0 rlineto closepath fill
	%brown
        0.7 0.3 0.2 setrgbcolor
	step 0 sqr
        %Orange
        1 0.5 0 setrgbcolor
        0 step sqr
     } def


/frame {step 7 div setlinewidth
	0 0 0 setrgbcolor
%	newpath 0 0 moveto 5 step mul 0 rlineto 0 4 step mul rlineto -5 step mul 0 rlineto 0 -4 step mul rlineto stroke
        0 1 4 { /i exch def
	newpath 0 i step mul moveto 5 step mul 0 rlineto stroke
	} for
        0 1 5 {/i exch def
	newpath i step mul 0 moveto 0 4 step mul rlineto stroke
	      } for
	step 14 div setlinewidth
	1 1 1 setrgbcolor
	newpath 0 0 moveto 2 step mul 0 rlineto 0 2 step mul rlineto -2 step mul 0 rlineto 0 -2 step mul rlineto stroke
	newpath 2 step mul 2 step mul moveto 3 step mul 0 rlineto 0 2 step mul rlineto -3 step mul 0 rlineto 0 -2 step mul rlineto stroke
       } def


/D00 {/y exch def /x exch def
	gsave
        x y translate
	0 setlinewidth
	fixed
	0 0 1 setrgbcolor	3 step mul 2 step mul sqr
	0 0.6 0 setrgbcolor	4 step mul 3 step mul sqr
	
	0 1 1 setrgbcolor	0 0 sqr
        1 0 1 setrgbcolor       step step sqr
	frame
	grestore
       } def

/D10 {/y exch def /x exch def
	gsave
        x y translate
	0 setlinewidth
	fixed 
	0 0 1 setrgbcolor	4 step mul 3 step mul sqr
	0 0.6 0 setrgbcolor	3 step mul 2 step mul sqr
	
	0 1 1 setrgbcolor	0 0 sqr
        1 0 1 setrgbcolor       step step sqr

	frame
	grestore
       } def

/D01 {/y exch def /x exch def
	gsave
        x y translate
	0 setlinewidth
	fixed
	0 0 1 setrgbcolor	3 step mul 2 step mul sqr
	0 0.6 0 setrgbcolor	4 step mul 3 step mul sqr
	
	0 1 1 setrgbcolor	step step sqr
        1 0 1 setrgbcolor       0 0 sqr
	frame
	grestore
       } def

/D11 {/y exch def /x exch def
	gsave
        x y translate
	0 setlinewidth
	fixed 
	0 0 1 setrgbcolor	4 step mul 3 step mul sqr
	0 0.6 0 setrgbcolor	3 step mul 2 step mul sqr
	
	0 1 1 setrgbcolor	step step sqr
        1 0 1 setrgbcolor       0 0 sqr

	frame
	grestore
       } def

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
1 setlinejoin
1 setlinecap


2 2 translate

0   0 step mul D00       6.5 step mul    0 step mul  D10
0 5.5 step mul D01       6.5 step mul  5.5 step mul  D11


showpage
grestore
end
%%EOF
