%!PS-Adobe-3.0 EPSF
%%Title: ScSt_parabolae.eps
%%Creator: Frank Sottile
%%Pages:  1
%%BoundingBox: 0 0 168 120 
%%EndComments

30 dict begin
1 setlinejoin
1 setlinecap

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%    Draws box from (a,b) to (c,d) 
/BOX {/d exch def /c exch def /b exch def /a exch def 
        a b moveto c b lineto c d lineto a d lineto closepath} def
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%50 150 translate 3 3 scale %1 1 0.6 setrgbcolor  0 0 112 80 BOX %stroke %fill

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
/sc 38 19 add def
/st 2 sqrt 1.00005 mul def
/int 2 3 div def
/dwp {/t exch def    t sc mul  1 t t mul sub sc mul } def
/upp {/t exch def    t sc mul  t t mul 1 sub sc mul } def
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
sc st mul 2.5 add   sc 2 add translate
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%
%  The regions
%
0 setlinewidth
0.5 1 1 setrgbcolor
newpath st dwp moveto
   st 0.999 st sub 10 div 1 {/t exch def  t dwp lineto } for
   1  st 1 sub 10 div st {/t exch def  t upp lineto } for st upp lineto 
 closepath fill   
newpath st neg dwp moveto
   st neg st 0.999 sub 10 div -1 {/t exch def  t dwp lineto } for
   -1  0.999 st sub 10 div st neg {/t exch def  t upp lineto } for    st neg upp lineto 
 closepath fill   
1 0.5 1 setrgbcolor
newpath  0 sc moveto 
 0 0.03333 1 {/t exch def  t dwp lineto } for
   1  st 1 sub 10 div st {/t exch def  t upp lineto } for  st upp lineto 
 closepath fill   
1 0.5 1 setrgbcolor
newpath  0 sc neg moveto 
 0 -0.03333 -1 {/t exch def  t upp lineto } for
   -1  1 st sub 10 div st neg {/t exch def  t dwp lineto } for  st neg dwp lineto 
 closepath fill   
1 1 0.5 setrgbcolor
newpath  0 sc neg moveto 
 0 0.03333 1 {/t exch def  t upp lineto } for
   1  st 1 sub 10 div st {/t exch def  t dwp lineto } for  st dwp lineto 
 closepath fill   
newpath  0 sc moveto 
 0 -0.03333 -1 {/t exch def  t dwp lineto } for
   -1  1 st sub 10 div st neg {/t exch def  t upp lineto } for  st neg upp lineto 
 closepath fill   
0.7 0.7 1 setrgbcolor
newpath -1 0 moveto   -1 0.033333 1 {/t exch def  t dwp lineto } for 1 0 lineto closepath fill 

0.5 1 0.5 setrgbcolor
newpath -1 0 moveto   -1 0.033333 1 {/t exch def  t upp lineto } for 1 0 lineto closepath fill
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
2 setlinewidth
1 1 1 setrgbcolor
 newpath st sc mul neg sc moveto st  sc mul sc lineto st  sc mul sc neg lineto st  sc mul neg sc neg lineto closepath stroke 
 1.6 setlinewidth
[1 2] 0 setdash

0.6 0.271 0.0745 setrgbcolor
newpath st sc mul neg sc moveto st  sc mul sc lineto st  sc mul sc neg lineto st  sc mul neg sc neg lineto closepath stroke 

[] 0 setdash
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%
%  The curves
%
0 0 1 setrgbcolor
newpath sc neg 0 moveto sc 0 lineto stroke
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
newpath int neg upp  moveto
 int st int sub 10 div st {/t exch def  t neg upp lineto } for stroke
newpath int neg upp  moveto
 int neg int 30 div int {/t exch def  t  upp lineto } for stroke
newpath int  upp  moveto
 int st int sub 10 div st {/t exch def  t upp lineto } for stroke

newpath int neg dwp  moveto
 int st int sub 10 div st {/t exch def  t neg dwp lineto } for stroke
newpath int neg dwp  moveto
 int neg int 30 div int {/t exch def  t  dwp lineto } for stroke
newpath int  dwp  moveto
 int st int sub 10 div st {/t exch def  t dwp lineto } for stroke
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
 

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%
%  Dots
%
/R 1.8 def
/r 1.5 def
0 0 0 setrgbcolor
0 setlinewidth
   sc neg       0      R 0 360 arc fill
  sc            0      R 0 360 arc fill


0.6 0.271 0.0745 setrgbcolor
st sc mul neg  sc      r 0 360 arc fill
  0            sc      r 0 360 arc fill
st sc mul      sc      r 0 360 arc fill
st sc mul neg  sc neg  r 0 360 arc fill
  0            sc neg  r 0 360 arc fill
st sc mul      sc neg  r 0 360 arc fill


%0.5 setlinewidth
%newpath -80 0 moveto 80 0 lineto stroke
%newpath 0 -60 moveto  0 60 lineto stroke

showpage
grestore
end
